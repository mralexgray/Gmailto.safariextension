<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
	<string>Patrick Gibson</string>
	<key>CFBundleDisplayName</key>
	<string>Gmailto:</string>
	<key>CFBundleIdentifier</key>
	<string>com.patrickgibson.gmailto</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1.0</string>
	<key>Chrome</key>
	<dict>
		<key>Global Page</key>
		<string>global.html</string>
	</dict>
	<key>Content</key>
	<dict>
		<key>Scripts</key>
		<dict>
			<key>End</key>
			<array>
				<string>gmailto.js</string>
			</array>
		</dict>
	</dict>
	<key>Description</key>
	<string>An extension to fix all mailto: links to open up in a Gmail window</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>All</string>
		</dict>
	</dict>
	<key>Update Manifest URL</key>
	<string>http://patrickgibson.com/projects/gmailto/gmailto.plist</string>
	<key>Website</key>
	<string>http://patrickgibson.com/projects/gmailto/</string>
</dict>
</plist>
